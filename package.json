{
  "name": "express-starter",
  "version": "0.0.1",
  "description": "express-starter",
  "main": "app.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./yarn.lock,./commitlint.config,./package-lock.json,./README.md --copy-files",
    "start": "npm run build && node dist/bin/www.js",
    "test": "jest",
    "check": "eslint .",
    "precheck": "prettier --write *.{js,json,html,css,md}"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint ."
    ],
    "*.{js,json,html,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "aihanjiao",
    "url": "https://github.con/aihanjiao"
  },
  "license": "",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "moment": "^2.26.0",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express-swagger-generator": "^1.1.17",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  }
}
